package com.kmove.app.user.vo;

import java.util.Date;

//유저가
public class UserVo {

	 	private String uidx ;
		//Chaebun을 활용한 Primary Key
	 	
	 	private String uname; // 유저 이름 
	 	private String uid; // 유저 ID
	 	private String upw; //유저 Password
	 	private Date ubirth; // 유저 생일 
	 	private boolean ugender; // 유저 성별 (Y-남/N-녀)
	 	private String uphone; // 유저의 전화번호 (010-****-****)
	 	private String upost;
	 	private String uaddr1;
	 	private String uaddr2;
	 	private String umail;
	 	private String udeleteyn;
	 	private String uinsertdate;
	 	private String uupdatedate;
	 	private String udeletedate;	 
}
