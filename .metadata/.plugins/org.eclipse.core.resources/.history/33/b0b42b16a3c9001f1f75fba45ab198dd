package com.kmove.app.user.vo;

import javax.xml.crypto.Data;

//유저가
public class UserVo {
	
	 	private String uName;      // 이름
	    private String uId;        // 아이디
	    private String uPw;        // 비밀번호
	    private Data uBir;       // 생년월일
	    private String uPh;        // 전화번호
	    private String uPo;        // 우편번호
	    private String uAddr1;     // 주소1
	    private String uAddr2;     // 주소2(상세주소)
	    private String uMail;      // 이메일
}
